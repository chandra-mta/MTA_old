        function WindowOpener(file) {
            msgWindow = open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=no,status=no,width=760,height=600,resize=no");
            msgWindow.document.clear();
            msgWindow.document.write("<html><head><title>Gyro Drift Plots</title></head>");
